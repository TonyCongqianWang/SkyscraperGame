<Window x:Class="SkyscraperGameGui.CellDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Insert" Height="220" Width="160"
        ResizeMode="NoResize"
        KeyDown="Window_KeyDown">
    <Grid>
		<Grid x:Name="OptionsGrid" HorizontalAlignment="Center" Height="125" Margin="0,10,0,0" VerticalAlignment="Top" Width="125" ShowGridLines="True"/>

		<Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="60" Height="25" FontSize="12" Click="CancelButton_Click"/>
    </Grid>
</Window>